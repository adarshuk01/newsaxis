{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs18.x",
        "includeFiles": "node_modules/**"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  