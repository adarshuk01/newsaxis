{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true", 
      "CHROME_BIN": "/opt/render/chrome", 
    }
  }
  