{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api"
      }
    ],
    "env": {
      "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
    }
  }
  
  